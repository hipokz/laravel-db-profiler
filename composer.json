{
    "name": "hipokz/db-profiler",
    "description": "DB profiling for Lumen web and console applications.",
    "keywords": ["lumen", "db", "database", "profiler", "profiling", "mysql"],
    "license": "MIT",
    "authors": [
        {
            "name": "Dmitry Ivanov",
            "email": "dmitry.g.ivanov@gmail.com"
        },
        {
            "name": "Yerzhan Torgayev",
            "email": "yerzhan.torgayev@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.7.*",
        "illuminate/database": "5.7.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "3.7.*",
        "illuminated/testing-tools": "5.7.*",
        "illuminated/helper-functions": "5.7.*"
    },
    "autoload": {
        "psr-4": {
            "Illuminated\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/DbProfiler/TestCase.php",
            "tests/DbProfiler/fixture/app"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Illuminated\\Database\\DbProfilerServiceProvider"
            ]
        }
    }
}
